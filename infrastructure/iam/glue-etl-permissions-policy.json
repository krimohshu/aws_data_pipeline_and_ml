{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3DataLakeAccess",
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:DeleteObject",
        "s3:ListBucket",
        "s3:GetBucketLocation"
      ],
      "Resource": [
        "arn:aws:s3:::data-lake-raw-zone-616129051451",
        "arn:aws:s3:::data-lake-raw-zone-616129051451/*",
        "arn:aws:s3:::data-lake-processed-zone-616129051451",
        "arn:aws:s3:::data-lake-processed-zone-616129051451/*",
        "arn:aws:s3:::data-lake-scripts-616129051451",
        "arn:aws:s3:::data-lake-scripts-616129051451/*"
      ]
    },
    {
      "Sid": "GlueCatalogAccess",
      "Effect": "Allow",
      "Action": [
        "glue:GetDatabase",
        "glue:GetTable",
        "glue:GetPartitions",
        "glue:CreateTable",
        "glue:UpdateTable",
        "glue:CreatePartition",
        "glue:UpdatePartition",
        "glue:BatchCreatePartition"
      ],
      "Resource": [
        "arn:aws:glue:eu-west-2:616129051451:catalog",
        "arn:aws:glue:eu-west-2:616129051451:database/data_pipeline_catalog",
        "arn:aws:glue:eu-west-2:616129051451:table/data_pipeline_catalog/*"
      ]
    },
    {
      "Sid": "CloudWatchLogsAccess",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": [
        "arn:aws:logs:eu-west-2:616129051451:log-group:/aws-glue/*"
      ]
    }
  ]
}
